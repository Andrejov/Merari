{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["test"],
            "name": "compile & launch",
            "sourceMaps": true,
            "program": "${workspaceFolder}/src/index.ts"
        },
        {
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "postinstall"],
            "name": "compile"
        },
        {
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["start"],
            "name": "launch dist"
        }
    ]
}